{
  "expo": {
    "name": "kalpx",
    "slug": "kalpx",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/AppIconImg.jpeg",
    "scheme": "kalpx",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.kalpx.app",
      "usesAppleSignIn": true,
      "appleTeamId": "9G5NZ5LBRU",
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "versionCode": 12,
      "package": "com.kalpx.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/AppIconImg.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
          "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "",
          "enableGooglePay": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.473187060791-96pucdifumqrnn7lb5l6bboqladmarat"
        }
      ],
      ["expo-apple-authentication"]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "7461904f-1bca-4590-bc3b-c839d2768e44"
      }
    },
    "scripts": {
      "postinstall": "patch-package"
    }
  }
}
