{
  "cli": {
    "version": ">= 0.53.0"
  },
  "build": {
    "production": {
      "env": {
        "EXPO_UPDATES_DISABLED": "1"
      },
      "ios": {
        "autoIncrement": true
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
